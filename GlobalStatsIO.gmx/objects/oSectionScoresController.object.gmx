draw_text(room_width/2, 30, "Section scores");
draw_set_font(fontNormal);

var yy = 100;
var xx = room_width/2;
var elems = 0;
var i;

// rank information
if (oGlobalStatsIOController.gsRank == 0) {
    draw_text(xx, yy, "Retrieving rank section information...");
} else {

    var name, rank, lastscore, lasttime;
    // draw better players
    draw_set_color(c_white);
    elems = oGlobalStatsIOController.gsBetterRanksSize;
    for (i = 0; i &lt; elems; i++) {
        var name, rank, lastscore;
        name = oGlobalStatsIOController.gsBetterRanks[i, RankResult.Name];
        rank = oGlobalStatsIOController.gsBetterRanks[i, RankResult.Rank];
        lastscore = oGlobalStatsIOController.gsBetterRanks[i, RankResult.Value];
        draw_text(xx, yy, "Rank " + string(rank) + ": " + string(name) + " with a score of " + string(lastscore));
        yy += 30;
    }
    // draw score for current player
    draw_set_color(c_lime);
    name = oGlobalStatsIOController.gsRank[RankResult.Name];
    rank = oGlobalStatsIOController.gsRank[RankResult.Rank];
    lastscore = oGlobalStatsIOController.gsRank[RankResult.Value];
    draw_text(xx, yy, "Rank " + string(rank) + ": " + string(name) + " with a score of " + string(lastscore));
    yy += 30;
    // draw worse players
    draw_set_color(c_white);
    elems = oGlobalStatsIOController.gsWorseRanksSize;
